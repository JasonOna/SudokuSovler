{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "target": "esnext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "strict": true
  }
}
